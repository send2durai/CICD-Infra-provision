pipeline {
  agent any

  environment {
    AWS_ACCESS_KEY_ID = credentials('jenkins-aws-access-key-id')
    AWS_SECRET_ACCESS_KEY = credentials('jenkins-aws-secret-access-key-id')
  }

  stages {
    stage("Shell Script is going to be invoked"){
      steps{
        sh '''
            echo "chaging to the cert directory"
            cd cert
            pwd
            ls -lrth
            sudo chmod +x tfa.sh
            ./tfa.sh
        '''
      }
    }
  }
}